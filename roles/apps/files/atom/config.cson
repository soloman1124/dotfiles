"*":
  "atom-beautify":
    apex: {}
    arduino: {}
    c: {}
    cfml: {}
    coffeescript: {}
    cpp: {}
    cs: {}
    css: {}
    d: {}
    ejs: {}
    erb: {}
    fortran: {}
    general:
      _analyticsUserId: "fe1ac079-8e71-46df-b882-b2054643085c"
      beautifyEntireFileOnSave: false
    gherkin: {}
    handlebars: {}
    html:
      end_with_newline: true
      indent_inner_html: true
    jade: {}
    java: {}
    js:
      indent_size: 4
    json:
      indent_size: 2
    jsx: {}
    latex: {}
    less: {}
    marko: {}
    mustache: {}
    objectivec: {}
    pawn: {}
    perl: {}
    php: {}
    python:
      default_beautifier: "yapf"
      formatter: "yapf"
    riot: {}
    ruby: {}
    rust: {}
    sass: {}
    scss: {}
    spacebars: {}
    sql: {}
    svg: {}
    swig: {}
    tss: {}
    twig: {}
    typescript: {}
    vala: {}
    visualforce: {}
    xml: {}
    xtemplate: {}
  "autocomplete-python-jedi":
    useSnippets: "all"
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "linter-elixirc"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "node_modules"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "solarized-dark-syntax"
    ]
  editor:
    autoIndent: false
    fontFamily: "Source Code Variable, Light"
    fontSize: 15
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    tabType: "soft"
  "exception-reporting":
    userId: "9bd7c5b9-b65d-005b-3b46-9c2d79a4476f"
  hex: {}
  linter: {}
  "linter-elixirc": {}
  "linter-jshint": {}
  "markdown-preview":
    breakOnSingleNewline: true
  "python-isort":
    checkOnSave: false
    isortPath: "/usr/local/bin/isort"
  "tree-view": {}
  welcome:
    showOnStartup: false
".go.source":
  editor:
    tabLength: 2
    tabType: "hard"
".js.source":
  editor:
    autoIndent: true
    tabLength: 2
".json.source":
  editor:
    tabLength: 2
".python.source":
  editor:
    autoIndent: true
    autoIndentOnPaste: true
    tabLength: 4
